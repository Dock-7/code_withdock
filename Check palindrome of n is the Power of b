#include <bits/stdc++.h>
using namespace std;

// Counts digits as a string key (e.g., "0110000010" for 128)
string digitCount(long long n) {
    string count(10, '0');
    while (n > 0) {
        count[n % 10]++;
        n /= 10;
    }
    return count;
}

bool reorderedPowerOfBase(long long n, int b) {
    string target = digitCount(n);

    long long val = 1;
    while (to_string(val).size() <= to_string(n).size()) {
        if (digitCount(val) == target) return true;
        val *= b;
    }
    return false;
}

<------------ for power of 2-------->
for(int i=0;i<32;i++){
    if(check(1<<i)==target) return true;
   }
return false;
}
